{
    "name": "chat-praktikum",
    "version": "1.0.0",
    "scripts": {
        "dev": "webpack serve --history-api-fallback",
        "build": "webpack --mode=production --node-env=production",
        "start": "webpack --mode=production --node-env=production && node server",
        "test": "jest --runInBand",
        "lint": "eslint . --fix",
        "stylelint": "stylelint **/*.scss",
        "tsc": "tsc --noEmit",
        "prepare": "husky install"
    },
    "engines": {
        "node": ">=18.0"
    },
    "targets": {
        "default": {
            "engines": {
                "browsers": ">= 30%"
            }
        }
    },
    "author": "",
    "license": "ISC",
    "description": "Chat app",
    "dependencies": {
        "handlebars": "4.7.7",
        "uuid": "9.0.0"
    },
    "devDependencies": {
        "@types/babel__core": "^7.20.0",
        "@types/jest": "^29.4.0",
        "@types/uuid": "9.0.0",
        "@typescript-eslint/eslint-plugin": "5.49.0",
        "@typescript-eslint/parser": "5.49.0",
        "@webpack-cli/generators": "3.0.1",
        "babel-jest": "^29.5.0",
        "css-loader": "6.7.3",
        "eslint": "8.32.0",
        "express": "4.18.2",
        "html-webpack-plugin": "5.5.0",
        "husky": "^8.0.3",
        "jest": "^29.5.0",
        "jest-environment-jsdom": "^29.5.0",
        "jest-localstorage-mock": "^2.4.26",
        "mini-css-extract-plugin": "2.7.2",
        "sass": "1.58.0",
        "sass-loader": "13.2.0",
        "style-loader": "3.3.1",
        "stylelint": "14.16.1",
        "stylelint-config-standard": "29.0.0",
        "stylelint-config-standard-scss": "6.1.0",
        "ts-jest": "^29.0.5",
        "ts-loader": "9.4.2",
        "typescript": "4.9.5",
        "webpack": "5.75.0",
        "webpack-cli": "5.0.1",
        "webpack-dev-server": "4.11.1"
    }
}
